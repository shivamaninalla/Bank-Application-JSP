package com.monocept.model;

public class Employee {
	
	private String name;
	private int id;
	private double salary;
	public Employee(String name, int id, double salary, boolean isManager) {
		super();
		this.name = name;
		this.id = id;
		this.salary = salary;
		this.isManager = isManager;
	}
	private boolean isManager;
	

}
